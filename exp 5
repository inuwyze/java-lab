import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Painting {
    
    String title,name,medium;
    int price;

    
    Painting(String title,String name,String medium,int price )
    {
        this.title=title;
        this.name=name;
        this.medium=medium;
        this.price=price;
    }
    
    public static void main(String[] args) {
       Scanner sc=new Scanner(System.in);
        String t=sc.nextLine();
        String n=sc.nextLine();
        String m=sc.nextLine();
        int p=sc.nextInt();
        
        Painting ob=new Painting(t,n,m,p);
        System.out.println("Paint Title is:"+ob.title
+"\nName of the artist is:"+ob.name
+"\nMedium of Paint is:"+ob.medium
+"\nPaint Price is:"+ob.price);
    }
}
